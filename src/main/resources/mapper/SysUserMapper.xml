<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.huanzi.qch.baseadmin.sys.sysuser.mapper.SysUserMapper">
    <select id="findByLoginName" resultType="cn.huanzi.qch.baseadmin.sys.sysuser.pojo.SysUser">
        select * from sys_user where login_name = #{username}
